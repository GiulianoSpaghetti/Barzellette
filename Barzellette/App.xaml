<Application x:Class="Barzellette.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Barzellette"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary x:Key="en-US" Source="en_US.xaml" />
            <ResourceDictionary x:Key="it-IT" Source="it_IT.xaml" />
            <ResourceDictionary x:Key="de-DE" Source="de_DE.xaml" />
            <ResourceDictionary x:Key="es-ES" Source="es_ES.xaml" />
            <ResourceDictionary x:Key="fr-FR" Source="fr_FR.xaml" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
